@model List<FirstProject.Models.Domain.Employee>
@{
    ViewData["Title"] = "Employee List";
}
<h1>
    Employees
</h1>
<table class="table">
    <thead> 
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Salary</th>
            <th>Date of Birth</th>
            <th>Department</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.Id</td>
                <td>@employee.Name</td>
                <td>@employee.Email</td>
                <td>@employee.Salary</td>
                <td>@employee.DOB.ToString("MM-d-yyyy")</td>
                <td>@employee.Department</td>
                <td><a href="Employee/View/@employee.Id">View</a></td>
            </tr>
        }
    </tbody>
</table>
